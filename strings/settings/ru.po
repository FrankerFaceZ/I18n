msgid "setting.entry.player.theatre.metadata"
msgstr "Показывать метаданные при наведении указателя мыши на проигрыватель."

msgid "setting.entry.player.theatre.no-whispers"
msgstr "Скрыть личные сообщения в режиме кинотеатра."

msgid "setting.entry.player.vod.autoplay"
msgstr ""
"Автоматическое воспроизведение следующего рекомендованного видео после его "
"завершения."

msgid "setting.entry.player.volume-always-shown"

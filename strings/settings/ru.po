msgstr "1%"

msgid "setting.entry.player.volume-scroll-steps.values.0.02"
msgstr "2%"

msgid "setting.entry.player.volume-scroll-steps.values.0.05"
msgstr "5%"

msgid "setting.entry.player.volume-scroll-steps.values.0.1"
msgstr "10%"

